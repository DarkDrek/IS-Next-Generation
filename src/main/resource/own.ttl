    ; foaf:name       "Jean-Luc Picard"^^xsd:string
    ; foaf:title      "Captain"^^xsd:string 
    ; sf:Dienstnummer "SP-937-215"
    ; foaf:member sf:Starfleet
    .

sf:Jean-Luc_Picard_Enrollment a bio:Enrollment 
    ; dc:date "2322" 
    ; bio:principal sf:Jean-Luc_Picard
    .

